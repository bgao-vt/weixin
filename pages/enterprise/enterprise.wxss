.category-block{
  width: 710rpx;
  background: #f5f5f5;
  padding: 20rpx;
}
.category-title{
  height: 50rpx;
  color: #333333;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  margin-left: -20rpx;
}
.list-section{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
.list-item {
  height: 200rpx;
  margin: 10rpx;
  border-radius: 8rpx;
  -webkit-box-shadow: 0px 1px 7px 0px rgba(51,51,51,0.4);
  -moz-box-shadow: 0px 1px 7px 0px rgba(51,51,51,0.4);
  box-shadow: 0px 1px 7px 0px rgba(51,51,51,0.4);
}
.list-item-clicked {
  height: 200rpx;
  margin: 10rpx;
  border-radius: 8rpx;
  -webkit-box-shadow: 0px 3px 7px 0px rgba(51,51,51,0.6);
  -moz-box-shadow: 0px 3px 7px 0px rgba(51,51,51,0.6);
  box-shadow: 0px 3px 7px 0px rgba(51,51,51,0.6);
}